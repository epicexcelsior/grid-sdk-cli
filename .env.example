# Grid SDK CLI Configuration
# Copy this file to .env and update with your actual values

# Grid Environment (sandbox or production)
GRID_ENVIRONMENT=sandbox

# Grid API Base URL (default: https://grid.squads.xyz)
GRID_BASE_URL=https://grid.squads.xyz

# Grid API Keys
# Get your API keys from: https://grid.squads.xyz/dashboard
GRID_SANDBOX_API_KEY=your_sandbox_api_key_here
GRID_PRODUCTION_API_KEY=your_production_api_key_here

# Debug Mode (true or false)
# When enabled, shows detailed logs and debug information
DEBUG=false